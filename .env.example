# API Keys for VPN Detection & De-anonymization Tool
# Copy this file to .env and fill in your actual API keys

# Google Gemini AI API Key
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# IPGeolocation API Key  
# Get your key from: https://ipgeolocation.io/
IPGEO_API_KEY=your_ipgeolocation_api_key_here

# VPN API Key
# Get your key from: https://vpnapi.io/
VPN_API_KEY=your_vpn_api_key_here

# Performance Settings (optional)
MAX_PACKETS=10000
CACHE_ENABLED=true
PARALLEL_PROCESSING=true
